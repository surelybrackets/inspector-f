{
  "name": "inspector-f",
  "version": "0.4.0",
  "description": "API for getting historicall stock/etf statistics.",
  "main": "index.ts",
  "scripts": {
    "init": "npm i & git config core.hooksPath hooks",
    "start": "npm run build && npm run server-up",
    "start:dev": "nodemon",
    "start:prod": "npm run build && docker-compose build && docker-compose up -d",
    "server-up": "node dist",
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "eslint ./src/**/*.ts",
    "preversion": "npm run test",
    "postversion": "git push --tags origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/surelybrackets/fin-ticker-historical-stats.git"
  },
  "author": "Matthew Bunch (bunch43@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/surelybrackets/fin-ticker-historical-stats/issues"
  },
  "homepage": "https://github.com/surelybrackets/fin-ticker-historical-stats#readme",
  "devDependencies": {
    "@types/csvtojson": "1.1.5",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.0",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.3.0",
    "nodemon": "2.0.2",
    "prettier": "2.0.4",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "axios": "0.19.2",
    "csvtojson": "2.0.10",
    "express": "4.17.1",
    "moment": "2.24.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start",
    "ext": "ts"
  }
}
